unit la_pila;

interface

uses
  classes, sysutils, LO_PILA;

  const
    bolillas = 75;

implementation

procedure generarBolilleroMezclado();
var
  i: integer;
  rd: tipodato;
begin
    //AbrirMe(MeBolillero);
    for i := 1 to bolillas+1 do
    begin
        rd.Numero := i;
        apilar(meBolillero,rd);
    end;
    //CerrarMe(MeBolillero);
end;


end.
